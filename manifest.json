{
  "name": "papi",
  "version": "1.0",
  "description": "chrome extension to check if the API behaviour is consistent",
  "devtools_page": "index.html",
  "manifest_version": 2,
  "web_accessible_resources": ["*"],
  "permissions": ["storage", "tabs", "webRequest"],
  "icons": {
    "16": "img/detective.png",
    "48": "img/detective.png",
    "128": "img/detective.png"
  }
}
